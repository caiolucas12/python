matriz = []
coluna = int(input())
operacao = input()

for i in range(12):
    linha = []
    for j in range(12):
        linha.append(float(input()))
    matriz.append(linha)

soma = 0

for i in range(12):
    soma += matriz[i][coluna]

resultado = soma
if operacao == 'M':
    resultado = soma / 12 

print(f'{resultado:.1f}')