texto = input()

minusculas = 0
maiusculas = 0
numeros = 0
especiais = 0
total = 0

for caracter in texto:
    if caracter.islower():
        minusculas += 1 
    elif caracter.isupper():
        maiusculas +=1 
    elif  caracter.isdigit():
        numeros += 1
    else:
        especiais += 1
    
print('Minúsculas: ', minusculas)
print("Maiúsculas:", maiusculas)
print("Números:", numeros)
print("Especiais:", especiais)
print("Total:", len(texto))